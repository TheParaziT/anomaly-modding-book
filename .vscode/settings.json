{
  "files.associations": {
    "docusaurus.config.js": "typescript",
    "sidebars.js": "typescript"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "on",
      "other": "on"
    }
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/build": true,
    "**/.docusaurus": true
  },
  "python-envs.pythonProjects": [
    {
      "path": "",
      "envManager": "ms-python.python:system",
      "packageManager": "ms-python.python:pip"
    }
  ]
}
